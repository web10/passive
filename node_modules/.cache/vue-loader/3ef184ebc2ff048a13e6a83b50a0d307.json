d\"\n    elevation=\"12\"\n    max-width=\"700\"\n  >\n    <v-card-title class=\"justify-center display-2 font-weight-light pt-5\">\n      Build your profile\n    </v-card-title>\n\n    <div class=\"text-center display-1 grey--text font-weight-light mb-6\">\n      This information will let us know more about you.\n    </div>\n\n    <v-tabs\n      ref=\"tabs\"\n      v-model=\"internalValue\"\n      background-color=\"green lighten-5\"\n      color=\"white\"\n      grow\n      slider-size=\"50\"\n    >\n      <v-tabs-slider\n        class=\"mt-1\"\n        color=\"success\"\n      />\n\n      <v-tab\n        v-for=\"(item, i) in items\"\n        :key=\"i\"\n        :ripple=\"false\"\n        :disabled=\"!availableSteps.includes(i)\"\n      >\n        {{ item }}\n      </v-tab>\n    </v-tabs>\n\n    <div class=\"my-6\" />\n\n    <v-card-text>\n      <v-tabs-items v-model=\"internalValue\">\n        <slot />\n      </v-tabs-items>\n    </v-card-text>\n\n    <v-card-actions class=\"pb-4 pa-4\">\n      <v-btn\n        :disabled=\"internalValue === 0\"\n        class=\"white--text\"\n        color=\"grey darken-2\"\n        min-width=\"125\"\n        @click=\"$emit('click:prev')\"\n      >\n        Previous\n      </v-btn>\n\n      <v-spacer />\n\n      <v-btn\n        :disabled=\"!availableSteps.includes(internalValue + 1)\"\n        color=\"success\"\n        min-width=\"100\"\n        @click=\"$emit('click:next')\"\n      >\n        {{ internalValue === items.length - 1 ? 'Finish' : 'Next' }}\n      </v-btn>\n    </v-card-actions>\n  </v-card>\n</template>\n\n<script>\n  // Mixins\n  import Proxyable from 'vuetify/lib/mixins/proxyable'\n\n  export default {\n    name: 'BaseMaterialWizard',\n\n    mixins: [Proxyable],\n\n    props: {\n      availableSteps: {\n        type: Array,\n        default: () => ([]),\n      },\n      items: {\n        type: Array,\n        default: () => ([]),\n      },\n    },\n  }\n</script>\n\n<style lang=\"sass\">\n  .v-card--wizard\n    overflow: visible\n\n    .v-tabs-bar\n      height: 56px\n      padding: 0 8px\n\n    .v-tabs-slider-wrapper\n      overflow: visible\n\n    .v-tabs-slider\n      border-radius: 4px\n\n    .v-tabs-slider-wrapper\n      contain: initial\n      z-index: 0\n</style>\n"]}]}