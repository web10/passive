;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwJA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA","file":"MaterialCard.vue","sourceRoot":"src/components/base","sourcesContent":["<template>\n  <v-card\n    v-bind=\"$attrs\"\n    :class=\"classes\"\n    class=\"v-card--material pa-3\"\n  >\n    <div class=\"d-flex grow flex-wrap\">\n      <v-avatar\n        v-if=\"avatar\"\n        size=\"128\"\n        class=\"mx-auto v-card--material__avatar elevation-12\"\n        color=\"grey\"\n      >\n        <v-img :src=\"avatar\" />\n      </v-avatar>\n\n      <v-sheet\n        v-else\n        :class=\"{\n          'pa-7': !$slots.image\n        }\"\n        :color=\"color\"\n        :max-height=\"icon ? 90 : undefined\"\n        :width=\"inline || icon ? 'auto' : '100%'\"\n        class=\"text-start v-card--material__heading mb-n6\"\n        dark\n      >\n        <slot\n          v-if=\"$slots.heading\"\n          name=\"heading\"\n        />\n\n        <slot\n          v-else-if=\"$slots.image\"\n          name=\"image\"\n        />\n\n        <div\n          v-else-if=\"title && !icon\"\n          class=\"display-1 font-weight-light\"\n          v-text=\"title\"\n        />\n\n        <v-icon\n          v-else-if=\"icon\"\n          size=\"32\"\n          v-text=\"icon\"\n        />\n\n        <div\n          v-if=\"text\"\n          class=\"headline font-weight-thin\"\n          v-text=\"text\"\n        />\n      </v-sheet>\n\n      <div\n        v-if=\"$slots['after-heading']\"\n        class=\"ml-6\"\n      >\n        <slot name=\"after-heading\" />\n      </div>\n\n      <v-col\n        v-if=\"hoverReveal\"\n        cols=\"12\"\n        class=\"text-center py-0 mt-n12\"\n      >\n        <slot name=\"reveal-actions\" />\n      </v-col>\n\n      <div\n        v-else-if=\"icon && title\"\n        class=\"ml-4\"\n      >\n        <div\n\n          class=\"card-title font-weight-light\"\n          v-text=\"title\"\n        />\n      </div>\n    </div>\n\n    <slot />\n\n    <template v-if=\"$slots.actions\">\n      <v-divider class=\"mt-2\" />\n\n      <v-card-actions class=\"pb-0\">\n        <slot name=\"actions\" />\n      </v-card-actions>\n    </template>\n  </v-card>\n</template>\n\n<script>\n  export default {\n    name: 'MaterialCard',\n\n    props: {\n      avatar: {\n        type: String,\n        default: '',\n      },\n      color: {\n        type: String,\n        default: 'success',\n      },\n      hoverReveal: {\n        type: Boolean,\n        default: false,\n      },\n      icon: {\n        type: String,\n        default: undefined,\n      },\n      image: {\n        type: Boolean,\n        default: false,\n      },\n      inline: {\n        type: Boolean,\n        default: false,\n      },\n      text: {\n        type: String,\n        default: '',\n      },\n      title: {\n        type: String,\n        default: '',\n      },\n    },\n\n    computed: {\n      classes () {\n        return {\n          'v-card--material--has-heading': this.hasHeading,\n          'v-card--material--hover-reveal': this.hoverReveal,\n        }\n      },\n      hasHeading () {\n        return Boolean(this.$slots.heading || this.title || this.icon)\n      },\n      hasAltHeading () {\n        return Boolean(this.$slots.heading || (this.title && this.icon))\n      },\n    },\n  }\n</script>\n\n<style lang=\"sass\">\n  .v-card--material\n    &__avatar\n      position: relative\n      top: -64px\n      margin-bottom: -32px\n\n    &__heading\n      position: relative\n      top: -40px\n      transition: .3s ease\n      z-index: 1\n\n    &.v-card--material--hover-reveal:hover\n      .v-card--material__heading\n        transform: translateY(-40px)\n</style>\n"]}]}